{
  "$schema": "http://json-schema.org/draft-04/schema#",

  "type" : "object",
  "id" : "TaskScheduledEvent",
  "properties" : {
    "updatedBy" : {
      "type" : "string"
    },
    "boardId" : {
      "type" : "string"
    },
    "updatedDate" : {
      "type" : "integer",
      "format" : "UTC_MILLISEC"
    },
    "status" : {
      "type" : "string",
      "enum" : [ "scheduled" ]
    }
  },
  "required": [
    "boardId",
    "updatedBy",
    "updatedDate",
    "status"
  ]
}
